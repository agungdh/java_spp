	<queryString>
		<![CDATA[SELECT p.*, s.nama, s.nis, s.jenis_kelamin,
concat(lpad(day(p.tanggal), 2, 0), '-', lpad(month(p.tanggal), 2, 0), '-', year(p.tanggal)) tanggalparsed
FROM siswa s, spp p
